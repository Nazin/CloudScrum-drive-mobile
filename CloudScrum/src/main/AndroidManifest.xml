<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pl.edu.agh.masters.cloudscrum"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.AUTHENTICATE_ACCOUNTS"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">

        <activity
            android:name="pl.edu.agh.masters.cloudscrum.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="pl.edu.agh.masters.cloudscrum.ProjectsActivity"
            android:label="@string/app_name" />

        <activity
            android:name="pl.edu.agh.masters.cloudscrum.ReleasesActivity"
            android:label="@string/app_name" />

        <activity
            android:name="pl.edu.agh.masters.cloudscrum.TasksActivity"
            android:label="@string/app_name" />

        <activity
            android:name="pl.edu.agh.masters.cloudscrum.TaskActivity"
            android:label="@string/app_name" />

        <activity
            android:name="pl.edu.agh.masters.cloudscrum.AllTasksActivity"
            android:label="@string/app_name" />

        <meta-data android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>
</manifest>
